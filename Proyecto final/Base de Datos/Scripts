Create database flaskcontacts;
Use flaskcontacts;

create table `user`
(
	id int AUTO_INCREMENT NOT NULL,
	cui int NOT NULL,
	password text NOT NULL,
	unique (cui),
	primary key(id)
);
create table horarios
(
	id int AUTO_INCREMENT NOT NULL,
	curso text NOT NULL,
	dia varchar(20) NOT NULL,
	hora_inicio time NOT NULL,
	hora_final time NOT NULL,
	profesor text NOT NULL,
	grupo varchar(3),
	PRIMARY KEY (id)
);

